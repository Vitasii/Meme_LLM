  },
  {
    "from": 311.529,
    "to": 313.029,
    "sid": 106,
    "location": 2,
    "content": "一个智慧的人",
    "music": 0
  },
  {
    "from": 313.029,
    "to": 314.96,
    "sid": 107,
    "location": 2,
    "content": "一定是一个谦卑的人",
    "music": 0
  },
  {
    "from": 314.979,
    "to": 316.719,
    "sid": 108,
    "location": 2,
    "content": "承认自己的无知",
    "music": 0
  },
  {
    "from": 316.719,
    "to": 318.759,
    "sid": 109,
    "location": 2,
    "content": "乃是开启智慧的大门",
    "music": 0
  },
